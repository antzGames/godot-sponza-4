; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

run/main_scene="res://sponza/sponza.tscn"
config/features=PackedStringArray("4.3")

[autoload]

UniversalSettings="*res://addons/universal_settings/scenes/universal_settings_menu.tscn"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/universal_settings/plugin.cfg")

[rendering]

textures/vram_compression/import_etc2_astc=true
global_illumination/sdfgi/probe_ray_count=5
global_illumination/sdfgi/frames_to_converge=0
